    <script>
        $(function () {
            getDestination();
        });

        function getDestination() {
            $.post('../ConTran/getDestination', function (data) {
                if (data == 0) {
                }
                else {
                    $('#div_Destination').html(data);
                }
            });
        }

        function getConName(destination) {
            $.post('../ConTran/getConName', { destination: destination }, function (data) {
                if (data == 0) {
                }
                else {
                    $('#div_ConName').html(data);
                }
            });
        }
        
        function Destination_Change() {
            var destination = document.getElementById("destination").value;
            getConName(destination);
        }

        function ConName_Change() {
            var destination = document.getElementById("destination").value;
            var conName = document.getElementById("conName").value;
            getConList(destination, conName);
        }

        function getConList(destination, conName) {
            $.post('../ConTran/getConList', { destination: destination, conName: conName }, function (data) {
                if (data == 0) {
                }
                else {
                    $('#TB_Feight').html(data);
                    getConCarrier();
                }
            });
        }

        function getConCarrier() {
            var destination = document.getElementById("destination").value;
            var conName = document.getElementById("conName").value;
            $.post('../ConTran/getConCarrier', { destination: destination, conName: conName }, function (data) {
                if (data == 0) {
                }
                else {
                    $('#conCarrier').val(data);
                }
            });
        }

        function inputConCarrier() {
            var weightIdList = $('#weightIdList').val();
            var conCarrierId = $('#conCarrier').val();
            window.location = '../ConTran/ConAdress?weightIdList=' + weightIdList + '&conCarrierId=' + conCarrierId;
        }

    </script>



